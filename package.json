{
  "name": "distube",
  "version": "3.0.0-beta.33",
  "description": "A Discord.js v12 module to simplify your music commands and play songs with audio filters on Discord without any API key. Support YouTube, SoundCloud, Bandcamp, Facebook, and 700+ more sites",
  "main": "src/DisTube.js",
  "typings": "typings/DisTube.d.ts",
  "scripts": {
    "docs": "docgen --source src --output docs/docs.json --custom pages/index.yml",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "types": "tsc -p .",
    "publish": "npm publish --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skick1234/DisTube.git"
  },
  "keywords": [
    "youtube",
    "music",
    "discord",
    "discordjs",
    "bot",
    "distube",
    "queue",
    "musicbot",
    "discord-music-bot",
    "music-bot",
    "discord-js"
  ],
  "author": "Skick (https://github.com/skick1234/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skick1234/DisTube/issues"
  },
  "funding": {
    "url": "https://patreon.com/DisTube"
  },
  "homepage": "https://distube.js.org/",
  "dependencies": {
    "@distube/youtube-dl": "^2.2.2",
    "@distube/ytpl": "^1.0.12",
    "@distube/ytsr": "^1.1.2",
    "ytdl-core": "^4.8.3"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.5.0",
    "@distube/docgen": "github:distubejs/docgen",
    "@types/node": "^16.0.0",
    "discord.js": "^12.5.3",
    "eslint": "^7.30.0",
    "eslint-plugin-jsdoc": "^35.4.1",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@discordjs/opus": "*",
    "discord.js": "12||13||^13.0.0-0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
