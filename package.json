{
  "name": "distube",
  "version": "3.0.0-alpha.8",
  "description": "A Discord.js module to simplify your music commands and play songs with audio filters on Discord without any API key. Support YouTube, SoundCloud, Bandcamp, Facebook, and 700+ more sites",
  "main": "dist/DisTube.js",
  "typings": "dist/DisTube.d.ts",
  "scripts": {
    "docgen": "npm run build && docgen --source dist --output docs/alpha.json --custom pages/index.yml",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write **/*.{ts,json,yml,yaml}",
    "build": "tsc",
    "build:check": "tsc --noEmit --incremental false",
    "publish:alpha": "npm publish --tag alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skick1234/DisTube.git"
  },
  "keywords": [
    "youtube",
    "music",
    "discord",
    "discordjs",
    "bot",
    "distube",
    "queue",
    "musicbot",
    "discord-music-bot",
    "music-bot",
    "discord-js"
  ],
  "author": "Skick (https://github.com/skick1234/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skick1234/DisTube/issues"
  },
  "funding": {
    "url": "https://patreon.com/DisTube"
  },
  "homepage": "https://distube.js.org/",
  "dependencies": {
    "@distube/youtube-dl": "^2.2.2",
    "@distube/ytpl": "^1.0.12",
    "@distube/ytsr": "^1.1.2",
    "eslint-plugin-jsdoc": "^35.4.1",
    "ytdl-core": "^4.8.3"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.5.0",
    "@discordjs/voice": "^0.5.1",
    "@distube/docgen": "github:distubejs/docgen",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "discord.js": "github:discordjs/discord.js#5ad83a6a65e5944ceb3a41fee2df40ba1f5b03e4",
    "eslint": "^7.29.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@discordjs/opus": "*",
    "@discordjs/voice": "^0.5.1",
    "discord.js": "12||13||^13.0.0-0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
